<div class="index">
<h1> Welcome to WDInstagram</h1>

<h3>Here are the photos we have:</h3>
<div>

<div class="all-photos">
<ul>
    
    <% @entries.each do |entry| %>
        <li> 
            <p class="author">Name: <%= entry.author %> <p>
            <p class="date">Name: <%= entry.date_taken %> <p>
            <p class="pphoto">Photo: <img class="photo" src="<%= entry.photo %>"> <p>
        </li>
    <% end %>
</ul>
</div>
